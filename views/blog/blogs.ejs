        <% blog.forEach(element => { %>
         
            <div class="col-lg-4 col-md-6">
            <article class="sigma_post style-15">
              <div class="sigma_post-thumb">
                <a href="/blog/<%= element.id %>">
                  <img src="<%= element.image %> " alt="img">
                </a>
              </div>
              <div class="sigma_post-body">
                <div class="sigma_post-content">
                  <div class="sigma_post-meta is-absolute">
                    <a href="/blog/<%= element.id %>" class="sigma_post-date">
  
                      <%= element.date.toISOString().split('T')[0] %>
  
                    </a>
                  </div>
                  <div class="sigma_post-meta">
                    <ul>
                      <li>
                        <a href="/blog/<%= element.id %>">
                          <i class="fal fa-user"></i>
                          By <%= element.writter%>
                        </a>
                      </li>
                      <li>
                        <a href="/blog/<%= element.id %>">
                          <i class="fal fa-folder-open"></i>
                          Air Transport
                        </a>
                      </li>
                    </ul>
                  </div>
                  <h5>
  
                    <a href="blog-details.html"><%= element.title %></a>
  
                  </h5>
                </div>
                <a href="/blog/<%= element.id %> " class="btn-link">
  
                  Read more
  
                </a>
              </div>
            </article>
          </div>  

  

